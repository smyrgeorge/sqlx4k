package = sqlx4k
headers = sqlx4k_postgres.h
compilerOpts = -I./rust_lib/target
staticLibraries = libsqlx4k_postgres.a
libraryPaths.ios_arm64 = ./rust_lib/target/aarch64-apple-ios/release
libraryPaths.android_x64 = ./rust_lib/target/aarch64-linux-android/release
libraryPaths.android_arm64 = ./rust_lib/target/aarch64-unknown-linux-gnu/release
libraryPaths.macos_arm64 = ./rust_lib/target/aarch64-apple-darwin/release
libraryPaths.macos_x64 = ./rust_lib/target/x86_64-apple-darwin/release
libraryPaths.linux_arm64 = ./rust_lib/target/x86_64-linux-android/release
libraryPaths.linux_x64 = ./rust_lib/target/x86_64-pc-windows-gnu/release
libraryPaths.minigw_x64 = ./rust_lib/target/x86_64-unknown-linux-gnu/release
