package = sqlx4k.postgresql
headers = sqlx4k_postgres.h
compilerOpts = -I./src/rust/target
staticLibraries = libsqlx4k_postgres.a
libraryPaths = ./src/rust/target/x86_64-pc-windows-gnu/release
# The following was found with: export RUSTFLAGS="--print=native-static-libs"
# https://github.com/HowardHinnant/date/issues/272
linkerOpts = -lntdll -lole32
# linkerOpts = -lsecur32 -lkernel32 -lbcrypt -ladvapi32 -lole32 -lkernel32 -ladvapi32 -lntdll -luserenv -lws2_32 -ldbghelp
# linkerOpts = -lsecur32 -lkernel32 -lbcrypt -ladvapi32 -lwindows.0.52.0 -lkernel32 -ladvapi32 -lntdll -luserenv -lws2_32 -ldbghelp
