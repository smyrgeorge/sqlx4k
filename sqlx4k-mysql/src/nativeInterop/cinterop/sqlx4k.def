package = sqlx4k.mysql
headers = sqlx4k_mysql.h
compilerOpts = -I./src/rust/target
linkerOpts.ios_arm64 = ./sqlx4k-mysql/src/rust/target/aarch64-apple-ios/release/libsqlx4k_mysql.dylib
