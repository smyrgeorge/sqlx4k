linkerOpts.macos_arm64 = ./sqlx4k-mysql/src/rust/target/aarch64-apple-darwin/release/libsqlx4k_mysql.dylib
linkerOpts.macos_x64 = ./sqlx4k-mysql/src/rust/target/x86_64-apple-darwin/release/libsqlx4k_mysql.dylib
linkerOpts.linux_arm64 = ./sqlx4k-mysql/src/rust/target/aarch64-unknown-linux-gnu/release/libsqlx4k_mysql.so
linkerOpts.linux_x64 = ./sqlx4k-mysql/src/rust/target/x86_64-unknown-linux-gnu/release/libsqlx4k_mysql.so
