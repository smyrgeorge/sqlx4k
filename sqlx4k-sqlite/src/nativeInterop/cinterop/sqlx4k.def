package = sqlx4k.sqlite
headers = sqlx4k_sqlite.h
compilerOpts = -I./src/rust/target
linkerOpts.ios_arm64 = ./sqlx4k-sqlite/src/rust/target/aarch64-apple-ios/release/libsqlx4k_sqlite.dylib
linkerOpts.ios_simulator_arm64 = ./sqlx4k-sqlite/src/rust/target/aarch64-apple-ios-sim/release/libsqlx4k_sqlite.dylib
linkerOpts.macos_arm64 = ./sqlx4k-sqlite/src/rust/target/aarch64-apple-darwin/release/libsqlx4k_sqlite.dylib
linkerOpts.macos_x64 = ./sqlx4k-sqlite/src/rust/target/x86_64-apple-darwin/release/libsqlx4k_sqlite.dylib
linkerOpts.linux_arm64 = ./sqlx4k-sqlite/src/rust/target/aarch64-unknown-linux-gnu/release/libsqlx4k_sqlite.so
linkerOpts.linux_x64 = ./sqlx4k-sqlite/src/rust/target/x86_64-unknown-linux-gnu/release/libsqlx4k_sqlite.so
